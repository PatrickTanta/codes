FoamFile
{
    version     2.0;
    format      ascii;
    class       volTensorField;
    object      K;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 0 0 0 0 0];

internalField   uniform (1e-12 0 0 0 1e-12 0 0 0 1e-12);

boundaryField
{
    openWall
    {
        type            zeroGradient;
    }
    
    endWall
    {
        type            zeroGradient;
    }

    fixedWalls
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}

