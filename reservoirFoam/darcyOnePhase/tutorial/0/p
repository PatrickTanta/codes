FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100E5;

boundaryField
{
    openWall
    {
        type            fixedValue;
        value           uniform 20E5;
    }
    
    endWall
    {
        type            fixedValue;
        value           uniform 100E5;
    }

    fixedWalls
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}

